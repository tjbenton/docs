header(class="s-header o-container")
  h1 Site header
  fieldset(class="c-settings js-settings")
    label(class="c-settings__state")
      | state
      input(class="js-settings__setting" type="checkbox" value="state")
    label(class="c-settings__designer")
      | designer
      input(class="js-settings__setting" type="checkbox" value="designer", checked)
    label(class="c-settings__developer")
      | developer
      input(class="js-settings__setting" type="checkbox" value="developer", checked)

