- var size = { header: 'h1', body: 'h2', inline: 'h4' }

#{size[token.blockinfo.comment.type]}(
  class="c-annotation c-annotation--name",
  id="#{id}"
)
  if token.access !== 'public'
    span(class='c-annotation c-annotation--access') [Private] 
  
  a(class="c-link-to js-link-to", href="##{id}")= data
