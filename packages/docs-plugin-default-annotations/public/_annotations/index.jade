mixin annotations(type, token) 
  if Array.isArray(token)
    each item in token
      +annotations(type, item)
  else
    - var elem, klass
    if type === 'header'
      - elem = type
      - klass = 'docs-c-token-header'
    else 
      - elem = 'section'
      - klass = 'docs-c-token-' + type
    #{elem}(class="#{klass}")
      h2 #{type} comment
      each data, annotation in token
        if annotation === 'inline'
          +annotations('inline', token.inline)
        else
          +annotation(annotation, data, token)
    
mixin annotation(name, data, obj)
  if name !== 'blockinfo' && name !== 'inline'
    if !!jade_mixins[name]
      +#{name}(data, obj)
    else 
      h1 #{name} doesn't exist as a mixin